<!doctype html>
<html ng-app="project">
 <head>
	<title><%=title%></title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular-resource.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular-route.min.js"></script>
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
	<link rel='stylesheet' href="css/style.css">
 </head>
<body>
<div id="log" style="display:none;"><%=parserLog%></div>


<div class="container" ng-controller="myCtrl">

<div ng-switch="showDiv" class="col-md-12">

<h2><%=title%></h2>

		<form class="form-inline">
		  <div class="form-group">
			<button class="btn btn-primary" ng-click="showDiv = !showDiv">Dados Json </button>
		  </div>
		  <div class="form-group text-right">
			<label for="search">Pesquisar</label>
			<input type="text" class="form-control" id="search" ng-model="search" placeholder="Pesquisar">
		  </div>
		 </form>

<div class="row">
	<div class="col-md-12" ng-switch-when=true>
		<div id="pre" class="col-md-12"></div>
	</div>
</div>

<div class="row">
	<div class="col-md-12" ng-switch-default>
		 
			<table class="table table-condesed">
				<thead>
					<tr>
					<th>Games</th>
					<th>Total Kills</th>
					<th>Players</th>
					<th>Kills</th>
					<th>Kills Means</th>
					</tr>
				</thead>
				<tbody ng-repeat="game in games | filter:search">
					<tr>
						<td><b>{{game.games}}</b></td>
						<td>{{game.total_kills}}</td>
						<td><table ng-repeat="(key, val) in game.players"><tr><td>{{val}}: {{key}}</tr></td></table></td>
						<td><table ng-repeat="(key, val) in game.kills"><tr><td>{{key}}: {{val}}</tr></td></table></td>
						<td><table ng-repeat="(key, val) in game.killsMeans"><tr><td>{{key}}: {{val}}</tr></td></table></td>
					</tr>
				</tbody>
			</table> 
	</div>
</div>
</div>
</div>

<script src="js/parserLog.js"></script>
<script src="js/project.js"></script>
</body>
</html>